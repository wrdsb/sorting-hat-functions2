{
    "bindings": [
        {
            "name": "triggerMessage",
            "type": "queueTrigger",
            "queueName": "memberships-legacy-calculate",
            "connection": "wrdsbsortinghat_STORAGE",
            "direction": "in"
        },
        {
            "name": "iamwpRaw",
            "type": "blob",
            "path": "test-data/view-iamwp-raw-test.json",
            "connection": "wrdsbsortinghat_STORAGE",
            "direction": "in"
        },
        {
            "name": "legacySetDefinition",
            "type": "cosmosDB",
            "databaseName": "sorting-hat",
            "collectionName": "people-set-definitions",
            "id": "{set}",
            "partitionKey": "{set}",
            "connectionStringSetting": "wrdsb-sorting-hat_CosmosDB",
            "direction": "in"
        },
        {
            "name": "setMembershipBlob",
            "type": "blob",
            "path": "legacy-set-memberships-now/{set}.json",
            "connection": "wrdsbsortinghat_STORAGE",
            "direction": "out"
        },
        {
            "name": "callbackMessage",
            "type": "queue",
            "queueName": "callbacks",
            "connection": "wrdsbsortinghat_STORAGE",
            "direction": "out"
        },
        {
            "name": "flynnEvent",
            "type": "queue",
            "queueName": "event-emitter",
            "connection": "wrdsbsortinghat_STORAGE",
            "direction": "out"
        }
    ],
    "disabled": false,
    "scriptFile": "../dist/memberships-legacy-calculate/index.js"
}